{
  "name": "eslint-plugin-mysticatea",
  "version": "5.0.0-beta.0",
  "description": "Additional ESLint rules.",
  "engines": {
    "node": ">=6.0.0"
  },
  "main": "index.js",
  "files": [
    "lib"
  ],
  "peerDependencies": {
    "eslint": ">=4.19.1"
  },
  "dependencies": {
    "eslint-plugin-eslint-comments": "~2.0.2",
    "eslint-plugin-eslint-plugin": "~1.4.0",
    "eslint-plugin-node": "~6.0.1",
    "eslint-plugin-prettier": "^2.6.0",
    "eslint-plugin-typescript": "~0.12.0",
    "eslint-plugin-vue": "~4.5.0",
    "globals": "^11.5.0",
    "prettier": "^1.12.1",
    "typescript-eslint-parser": "^15.0.0",
    "vue-eslint-parser": "^2.0.3"
  },
  "devDependencies": {
    "chokidar-cli": "^1.2.0",
    "codecov": "^3.0.1",
    "eslint": "^4.19.1",
    "eslint-config-prettier": "^2.9.0",
    "mocha": "^5.1.1",
    "npm-run-all": "^4.1.2",
    "nyc": "^11.7.1",
    "opener": "^1.4.3",
    "rimraf": "^2.6.2"
  },
  "scripts": {
    "preversion": "run-s clean update test",
    "version": "eslint lib/rules --fix && git add lib/rules",
    "postversion": "git push && git push --tags",
    "clean": "rimraf .nyc_output coverage",
    "lint": "eslint lib scripts tests .eslintrc.js",
    "test": "nyc mocha \"tests/lib/**/*.js\" --reporter dot",
    "update": "node scripts/generate-configs && node scripts/generate-rules",
    "watch": "chokidar lib tests --initial --command \"npm test && nyc report --reporter lcov\"",
    "coverage": "nyc report --reporter lcov && opener coverage/lcov-report/index.html",
    "codecov": "nyc report --reporter lcovonly && codecov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mysticatea/eslint-plugin.git"
  },
  "keywords": [
    "eslint",
    "eslintplugin",
    "block",
    "scope",
    "instanceof",
    "isArray",
    "static",
    "this",
    "rest",
    "spread",
    "ignore"
  ],
  "author": "Toru Nagashima",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mysticatea/eslint-plugin/issues"
  },
  "homepage": "https://github.com/mysticatea/eslint-plugin#readme"
}
